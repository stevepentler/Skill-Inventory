<div class="row"> 
  <div class="col s5"><img src="../images/Napoleon.jpg"></div>
  <div class="col s7">
  <h1>SWEET COMPUTER SKILLS</h1> 
  <ul><a id="add_skills" class="waves-effect waves-light btn-large co" href="/skills/new"><i class="material-icons left"></i>Add Your Own Skills</a></ul>
  <% @skills.each do |skill| %>
    <h3><%= skill.id %>: <a href="/skills/<%= skill.id %>">
    <%= skill.name %></a></h3>
    <h4><%= skill.status %></h4>
      <a href="/skills/<%= skill.id %>/edit">edit</a>
    <form action="/skills/<%= skill.id %>" method="post">
      <input class="waves-effect waves-light btn-large co" type="hidden" name="_method" value="delete">
      <input class="waves-effect waves-light btn-large co" type="submit" value="delete" />
    </form> 
  <% end %>
  </div>
</div>